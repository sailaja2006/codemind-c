#include <stdio.h>

int main(void) {
	// your code goes here
    int t;
    scanf ("%d", &t);
    for (int i = 0; i < t; i++) {
        int x;
        scanf ("%d", &x);
        if(x <= 100) {
            printf ("%d\n", x);
        } else {
            printf ("%d\n", x - 10);
        }
    }
    return 0;
}
