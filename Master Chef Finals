#include <stdio.h>

int main(void) {
    int T, X;
    scanf("%d", &T); // Read the number of test cases

    for (int i = 0; i < T; i++) {
        scanf("%d", &X); // Read X for each test case
        
        if (X <= 10) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }

    return 0;
}
